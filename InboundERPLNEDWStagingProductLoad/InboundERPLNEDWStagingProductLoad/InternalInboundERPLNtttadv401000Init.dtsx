      DTS:Expression="&quot;&#xA;/*&#xA;   r_currency_rates approval_date &amp; effective_date  Update Code &#xA;&#xA;   Script By Darlington C. Nwemeh Date: 10/23/2017 Time:2:00PM &#xA;&#xA;   Reason: Update approval_date &amp; effective_date with the Converted DayLight Savings Time (Accurate UTC Date)&#xA;&#xA;   Copyright(c) 2017 Quidel Corporation &#xA;*/&#xA;&#xA;DECLARE @CR_DateUpdate TABLE &#xA;(&#xA;&#x9;currency_base          [varchar](3) NULL,&#xA;&#x9;currency               [varchar](3) NULL,&#xA;&#x9;exchange_rate_type     [varchar](3) NULL,&#xA;&#x9;currency_rate          [float] NULL,&#xA;&#x9;rate_factor            [int] NULL,&#xA;&#x9;exp_base_currency      [int] NULL,&#xA;&#x9;currency_rate_approved [int] NULL,&#xA;&#x9;effective_date         [datetime] NULL,&#xA;&#x9;approval_date          [datetime] NULL&#xA;)&#xA;&#xA;&#xA;;WITH cte_currency_rates  &#xA;AS &#xA;(&#xA;SELECT  currency_base&#xA;      , currency&#xA;      , exchange_rate_type&#xA;      , currency_rate&#xA;      , rate_factor&#xA;      , exp_base_currency&#xA;      , currency_rate_approved&#xA;      , effective_date = &#xA;&#xA;  ( CASE&#xA;       WHEN A.effective_date  &gt; (SELECT dbo.GetDstEnd(DATEPART(YEAR, DATEADD(YEAR, -1, A.effective_date)))) &#xA;&#x9;    AND A.effective_date  &lt; (SELECT dbo.GetDstStart(DATEPART(YEAR, A.effective_date))) THEN DATEADD(hh, -8, A.effective_date)&#xA;       WHEN A.effective_date  &gt; (SELECT dbo.GetDstStart(DATEPART(YEAR, A.effective_date))) &#xA;&#x9;    AND A.effective_date  &lt; (SELECT dbo.GetDstEnd(DATEPART(YEAR, A.effective_date)))   THEN DATEADD(hh, -7, A.effective_date)&#xA;       WHEN A.effective_date  &gt; (SELECT dbo.GetDstEnd(DATEPART(YEAR, A.effective_date)))  &#xA;&#x9;    AND A.effective_date  &lt; (SELECT dbo.GetDstEnd(DATEPART(YEAR, DATEADD(YEAR, 1, A.effective_date)))) THEN DATEADD(hh, -8, A.effective_date)&#xA;       END) &#xA;&#x9;   , approval_date = &#xA; ( CASE&#xA;      WHEN A.approval_date &gt; (SELECT dbo.GetDstEnd(DATEPART(YEAR, DATEADD(YEAR, -1, A.approval_date)))) &#xA;&#x9;   AND A.approval_date &lt; (SELECT dbo.GetDstStart(DATEPART(YEAR, A.approval_date))) THEN DATEADD(hh, -8, A.approval_date)&#xA;      WHEN A.approval_date &gt; (SELECT dbo.GetDstStart(DATEPART(YEAR, A.approval_date))) &#xA;&#x9;   AND A.approval_date &lt; (SELECT dbo.GetDstEnd(DATEPART(YEAR, A.approval_date)))   THEN DATEADD(hh, -7, A.approval_date)&#xA;      WHEN A.approval_date &gt; (SELECT dbo.GetDstEnd(DATEPART(YEAR, A.approval_date)))  &#xA;&#x9;   AND A.approval_date &lt; (SELECT dbo.GetDstEnd(DATEPART(YEAR, DATEADD(YEAR, 1, A.approval_date)))) THEN DATEADD(hh, -8, A.approval_date)&#xA;       END) &#xA;&#xA;   FROM  EDW_STAGE.stage.r_currency_rates A &#xA;&#xA;&#xA;)&#xA;&#xA;INSERT INTO @CR_DateUpdate&#xA;&#xA;SELECT  currency_base&#xA;      , currency&#xA;      , exchange_rate_type&#xA;      , currency_rate&#xA;      , rate_factor&#xA;      , exp_base_currency&#xA;      , currency_rate_approved&#xA;      , effective_date &#xA;&#x9;  , approval_date&#xA; FROM cte_currency_rates  &#xA;&#xA;&#xA; -- Update Currency_Rates  effective_date Column&#xA; UPDATE C&#xA;    SET C.effective_date = CR.effective_date&#xA;   FROM stage.r_currency_rates C , @CR_DateUpdate CR&#xA;  WHERE CR.exchange_rate_type = C.exchange_rate_type&#xA;    AND CR.currency_rate = C.currency_rate&#xA;    AND CR.currency_rate_approved = C.currency_rate_approved&#xA;&#xA;&#xA; -- Update Currency_Rates approval_date Column&#xA; UPDATE C&#xA;    SET C.approval_date = CR.approval_date&#xA;   FROM stage.r_currency_rates C , @CR_DateUpdate CR&#xA;  WHERE CR.exchange_rate_type = C.exchange_rate_type&#xA;    AND CR.currency_rate = C.currency_rate&#xA;    AND CR.currency_rate_approved = C.currency_rate_approved&#xA;&#xA;&quot;"
