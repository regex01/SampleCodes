            {
                return false;
            }

            foreach (string filteredVariableName in maskedVariableNames)
            {
                if (string.IsNullOrEmpty(filteredVariableName))
                {
                    continue;
                }

                string trimmedFilteredVariableName = filteredVariableName.Trim();

