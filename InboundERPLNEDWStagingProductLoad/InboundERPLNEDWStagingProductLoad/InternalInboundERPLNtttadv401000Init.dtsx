    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{2DBEE549-659A-4888-AF98-AE1CE33D6335}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;/*  ** WORKING COPY **&#xA;   Shipments Daily Constant Currency Script By Darlington C. Nwemeh Version 1.1  Date:29/01/2018&#x9;&#xA;&#xA;   Reason: Add Constant Currency Workflow (LocalCurrency Amount, InvoiceAmount &amp; Revenue-Amounts Calculation) to Shipments Daily for Prior Year Currency Rates.&#xA;    &#xA;&#xA;   Copyright (c) 2018 Quidel Corporation. &#xA;*/ &#xA;&#xA;;WITH cte_Constant_Currency_Conversions &#xA;AS &#xA;(&#xA;SELECT  &#xA;        constant_currency_base                   AS Constant_Currency_Base&#xA;      , constant_currency                        AS Constant_Currency&#xA;      , constant_exchange_rate_type              AS Constant_Exchange_Rate_Type&#xA;      , constant_effective_date                  AS Constant_Effective_Date&#xA;      , constant_currency_rate                   AS Constant_Currency_Rate&#xA;      , constant_rate_factor                     AS Constant_Rate_Factor&#xA;      , constant_exp_base_currency               AS Exp_Base_Currency&#xA;      , constant_currency_rate_approved          AS Constant_Currency_Rate_Approved&#xA;      , constant_approval_date                   AS Constant_Approval_Date&#xA;  FROM EDW_STAGE.stage.r_constant_currency_rates&#xA; WHERE Constant_Currency_Base      = 'USD' &#xA;   AND Constant_Exchange_Rate_Type = '002' &#xA;&#xA;), &#xA;&#xA;cte_Currency_Conversions &#xA;AS &#xA;(&#xA;SELECT  currency_base                   AS Currency_Base&#xA;      , currency                        AS Currency&#xA;      , exchange_rate_type              AS Exchange_Rate_Type&#xA;      , effective_date                  AS Effective_Date&#xA;      , currency_rate                   AS Currency_Rate&#xA;      , rate_factor                     AS Rate_Factor&#xA;      , exp_base_currency               AS Exp_Base_Currency&#xA;      , currency_rate_approved          AS Currency_Rate_Approved&#xA;      , approval_date                   AS Approval_Date&#xA;  FROM EDW_STAGE.stage.r_currency_rates&#xA; WHERE currency_base = 'USD' &#xA;   AND Exchange_Rate_Type = '002' &#xA;&#xA;), &#xA;&#xA;cte_country_currency &#xA;AS&#xA;(&#xA; SELECT  country_id&#xA;       , country&#xA;&#x9;   , local_currency&#xA;&#x9;   , effective_date&#xA; FROM EDW_STAGE.stage.country_currency &#xA; &#xA; ), &#xA;&#xA;&#xA;cte_shipments_daily &#xA;AS&#xA;(&#xA;SELECT  DISTINCT&#xA;        CAST([ShipDate] AS DATE)              AS ShipDate&#xA;&#x9;  , cc.Country_ID                         AS Country_ID&#xA;&#x9;  , Billto_Country                        AS Billing_Company &#xA;      , ISNULL(cust.Customer_LN__SK,'99999')  AS Customer_LN_SK&#xA;&#x9;  , ISNULL(w.Warehouse_SK,'99999')        AS Warehouse_SK&#xA;      , CAST([InvoiceDate] AS DATE)           AS InvoiceDate&#xA;      , prod.[Product_SK]                     AS Product_SK&#xA;&#x9;  , cmp.Company_LN__SK                    AS Company_LN__SK&#xA;&#x9;  , ISNULL(ord.Order_Type_SK,'999')       AS Order_Type_SK&#xA;      , [Invoice#]                            AS Invoice#&#xA;&#x9;  , [OrderNo]                             AS OrderNo&#xA;&#x9;  , [InvoiceType]                         AS InvoiceType&#xA;      , [Kit_Quantity]                        AS Kit_Quantity&#xA;&#x9;  , cc.local_currency                     AS Local_Currency &#xA;      , fex1.[Trade_Currency]                 AS Trade_Currency&#xA;      , fex1.[Trade_Currency_Amount]          AS Trade_Currency_Amount&#xA;&#x9;  , fex1.Trade_Rate                       AS Trade_Rate &#xA;      , [Invoice_Amount]                      AS Invoice_Amount&#xA;      , ISNULL([Rebates_Discounts],0)         AS Rebates_Discounts&#xA;      , [Revenue_Amount]                      AS Revenue_Amount&#xA;      , ISNULL([Ext_Cost],0)                  AS Ext_Cost&#xA;      , ISNULL([Test_Volume],0)               AS Test_Volume&#xA;      , fex1.LeaseType                        AS LeaseType&#xA;      , fex1.Warehouse_ID                     AS Warehouse_ID&#xA;  FROM stage.[dbo].[F_Shipments_Fex2] fex1 &#xA;  LEFT JOIN edw_prod.[dbo].[D_Customer_LN] cust    ON ISNULL(fex1.SoldToCustomerID,'99999') = cust.Customer_ID&#xA;  LEFT JOIN edw_prod.[dbo].[D_Product] prod        ON LTRIM(fex1.Product_ID) = LTRIM(prod.[Product_ID]) &#xA;  LEFT JOIN edw_prod.[dbo].[D_Company_LN] cmp      ON fex1.[CompanyID] = cmp.Company_ID &#xA;  LEFT JOIN edw_prod.[dbo].[D_Order_Type] ord      ON fex1.[OrderType] = ord.Order_Type_Key&#xA;  LEFT JOIN edw_prod.[dbo].[D_Warehouse] w         ON fex1.[Warehouse_ID] = w.warehouse_id&#xA;  LEFT JOIN cte_country_currency cc                ON cc.country_id = Billto_Country COLLATE Latin1_General_CI_AS&#xA;  WHERE fex1.SoldToCustomerID NOT IN  ('CUS092228','CUS092229', 'CUS092230','CUS092231','CUS092232','CUS092237','CUS092239','CUS092233','CUS092238','CUS092234','CUS092235')&#xA;&#xA;  &#xA;  ), &#xA;&#xA;  cte_shipments_daily_main &#xA;  AS&#xA;  (&#xA;   SELECT &#xA;         ShipDate                 AS ShipDate&#xA;       , Customer_LN_SK           AS Customer_LN_SK&#xA;       , Warehouse_SK             AS Warehouse_SK&#xA;       , InvoiceDate              AS InvoiceDate&#xA;       , Product_SK               AS Product_SK&#xA;       , Company_LN__SK           AS Company_LN__SK&#xA;       , Order_Type_SK            AS Order_Type_SK&#xA;       , Invoice#                 AS Invoice#&#xA;       , OrderNo                  AS OrderNo&#xA;       , InvoiceType              AS InvoiceType&#xA;       , Kit_Quantity             AS Kit_Quantity&#xA;-- Local Currency Changes..  &#xA;&#x9;   , Local_Currency           AS Local_Currency&#xA;       , Trade_Currency           AS Trade_Currency&#xA;       , Trade_Currency_Amount    AS Trade_Currency_Amount&#xA;       , Trade_Rate               AS Trade_Rate&#xA;&#x9;  , CASE WHEN Local_Currency   = 'USD' THEN 1 &#xA;&#x9;         WHEN Local_Currency  != 'USD' THEN&#x9;                                &#xA;&#x9;   (SELECT Top 1 c.Constant_Currency_Rate FROM cte_Constant_Currency_Conversions c &#xA;&#x9;&#x9;WHERE  Local_Currency = c.Constant_Currency COLLATE Latin1_General_CI_AS &#xA;&#x9;&#x9;  AND CAST(c.Constant_Effective_Date AS DATE) &lt;= CAST(InvoiceDate AS DATE)&#xA;&#x9;&#x9;ORDER BY c.Constant_Effective_Date &#xA;&#x9;&#x9;DESC)  END                AS Constant_Conversion_Rate &#xA;       , Invoice_Amount           AS Invoice_Amount       &#xA;       , Rebates_Discounts        AS Rebates_Discounts&#xA;       , Revenue_Amount           AS Revenue_Amount&#xA;       , Ext_Cost                 AS Ext_Cost&#xA;       , Test_Volume              AS Test_Volume&#xA;       , LeaseType                AS LeaseType&#xA;       , Warehouse_ID             AS Warehouse_ID&#xA; FROM cte_shipments_daily&#xA;&#xA;  ), &#xA;&#xA;  cte_shipments_daily_calculated &#xA;  AS&#xA;  (&#xA;  &#xA; SELECT &#xA;         ShipDate                 AS ShipDate&#xA;       , Customer_LN_SK           AS Customer_LN_SK&#xA;       , Warehouse_SK             AS Warehouse_SK&#xA;       , InvoiceDate              AS InvoiceDate&#xA;       , Product_SK               AS Product_SK&#xA;       , Company_LN__SK           AS Company_LN__SK&#xA;       , Order_Type_SK            AS Order_Type_SK&#xA;       , Invoice#                 AS Invoice#&#xA;       , OrderNo                  AS OrderNo&#xA;       , InvoiceType              AS InvoiceType&#xA;       , Kit_Quantity             AS Kit_Quantity&#xA;       , Local_Currency           AS Local_Currency&#xA;&#x9;  , CASE WHEN Local_Currency  = 'USD' THEN 1 &#xA;&#x9;         WHEN Local_Currency != 'USD' THEN &#xA;&#x9;  ( SELECT Top 1 c.Currency_Rate FROM cte_Currency_Conversions c &#xA;&#x9;&#x9;WHERE  Local_Currency = c.Currency COLLATE Latin1_General_CI_AS &#xA;&#x9;&#x9;  AND  CAST(c.Effective_Date AS DATE) &lt;= CAST(InvoiceDate AS DATE)&#xA;&#x9;&#x9;ORDER BY c.Effective_Date &#xA;&#x9;&#x9;DESC) END                 AS Conversion_Rate&#xA;       , Trade_Currency           AS Trade_Currency&#xA;       , Trade_Currency_Amount    AS Trade_Currency_Amount&#xA;       , Trade_Rate               AS Trade_Rate&#xA;&#x9;   , Constant_Conversion_Rate AS Constant_Conversion_Rate&#xA;       , (Trade_Currency_Amount * Constant_Conversion_Rate) &#xA;&#x9;                              AS Constant_Invoice_Amount&#xA;       , Invoice_Amount           AS Invoice_Amount       &#xA;       , Rebates_Discounts        AS Rebates_Discounts&#xA;       , Revenue_Amount           AS Revenue_Amount&#xA;       , Ext_Cost                 AS Ext_Cost&#xA;       , Test_Volume              AS Test_Volume&#xA;       , LeaseType                AS LeaseType&#xA;       , Warehouse_ID             AS Warehouse_ID&#xA; FROM cte_shipments_daily_main&#xA;&#xA;  ), &#xA;&#xA;  cte_shipments_daily_calculated2&#xA;  AS &#xA;  (&#xA;   SELECT  &#xA;         ShipDate                 AS ShipDate&#xA;       , Customer_LN_SK           AS Customer_LN_SK&#xA;       , Warehouse_SK             AS Warehouse_SK&#xA;       , InvoiceDate              AS InvoiceDate&#xA;       , Product_SK               AS Product_SK&#xA;       , Company_LN__SK           AS Company_LN__SK&#xA;       , Order_Type_SK            AS Order_Type_SK&#xA;       , Invoice#                 AS Invoice#&#xA;       , OrderNo                  AS OrderNo&#xA;       , InvoiceType              AS InvoiceType&#xA;       , Kit_Quantity             AS Kit_Quantity&#xA;       , Local_Currency           AS Local_Currency&#xA;       , (Invoice_Amount * Conversion_Rate)                     &#xA;&#x9;                              AS Local_Currency_Amount&#xA;       , Conversion_Rate          AS Conversion_Rate&#xA;       , Trade_Currency           AS Trade_Currency&#xA;       , Trade_Currency_Amount    AS Trade_Currency_Amount&#xA;       , Trade_Rate               AS Trade_Rate&#xA;&#x9;   , Constant_Conversion_Rate AS Constant_Conversion_Rate&#xA;&#x9;   , (Trade_Currency_Amount / Constant_Conversion_Rate)&#xA;&#x9;                              AS Constant_Invoice_Amount&#xA;       , Invoice_Amount           AS Invoice_Amount           &#xA;       , Rebates_Discounts        AS Rebates_Discounts&#xA;&#x9;   , Revenue_Amount           AS Revenue_Amount&#xA;       , Ext_Cost                 AS Ext_Cost&#xA;       , Test_Volume              AS Test_Volume&#xA;       , LeaseType                AS LeaseType&#xA;       , Warehouse_ID             AS Warehouse_ID&#xA; FROM cte_shipments_daily_calculated&#xA;&#xA;  ), &#xA;&#xA;  cte_shipments_daily_final &#xA;  AS&#xA;  (&#xA;   SELECT  &#xA;         ShipDate                 AS ShipDate&#xA;       , Customer_LN_SK           AS Customer_LN_SK&#xA;       , Warehouse_SK             AS Warehouse_SK&#xA;       , InvoiceDate              AS InvoiceDate&#xA;       , Product_SK               AS Product_SK&#xA;       , Company_LN__SK           AS Company_LN__SK&#xA;       , Order_Type_SK            AS Order_Type_SK&#xA;       , Invoice#                 AS Invoice#&#xA;       , OrderNo                  AS OrderNo&#xA;       , InvoiceType              AS InvoiceType&#xA;       , Kit_Quantity             AS Kit_Quantity&#xA;       , Local_Currency           AS Local_Currency&#xA;       , Local_Currency_Amount    AS Local_Currency_Amount                                       &#xA;       , Conversion_Rate          AS Conversion_Rate&#xA;       , Trade_Currency           AS Trade_Currency&#xA;       , Trade_Currency_Amount    AS Trade_Currency_Amount&#xA;       , Trade_Rate               AS Trade_Rate&#xA;&#x9;   , Constant_Conversion_Rate AS Constant_Conversion_Rate&#xA;&#x9;   , (Local_Currency_Amount / Constant_Conversion_Rate)&#xA;&#x9;                              AS Constant_Invoice_Amount&#xA;       , Invoice_Amount           AS Invoice_Amount           &#xA;       , Rebates_Discounts        AS Rebates_Discounts&#xA;&#x9;   , Revenue_Amount           AS Revenue_Amount&#xA;       , Ext_Cost                 AS Ext_Cost&#xA;       , Test_Volume              AS Test_Volume&#xA;       , LeaseType                AS LeaseType&#xA;       , Warehouse_ID             AS Warehouse_ID&#xA; FROM cte_shipments_daily_calculated2&#xA;&#xA; )&#xA;&#xA; SELECT &#xA;         ShipDate              &#xA;       , Customer_LN_SK       &#xA;       , Warehouse_SK         &#xA;       , InvoiceDate           &#xA;       , Product_SK            &#xA;       , Company_LN__SK       &#xA;       , Order_Type_SK         &#xA;       , Invoice#              &#xA;       , OrderNo              &#xA;       , InvoiceType           &#xA;       , Kit_Quantity          &#xA;       , Local_Currency        &#xA;       , Local_Currency_Amount &#xA;       , Conversion_Rate &#xA;       , Trade_Currency            &#xA;       , Trade_Currency_Amount     &#xA;       , Trade_Rate               &#xA;&#x9;   , Constant_Conversion_Rate  &#xA;       , Constant_Invoice_Amount &#xA;&#x9;   ,(Constant_Invoice_Amount - Rebates_Discounts)  AS Constant_Revenue_Amount   &#xA;       , Invoice_Amount          &#xA;       , Rebates_Discounts     &#xA;       , Revenue_Amount                       &#xA;       , Ext_Cost             &#xA;       , Test_Volume         &#xA;       , LeaseType           &#xA;       , Warehouse_ID           &#xA;  FROM cte_shipments_daily_final&#xA;  --WHERE OrderNo = 'SI1005006'&quot;"
      DTS:IncludeInDebugDump="2345"
