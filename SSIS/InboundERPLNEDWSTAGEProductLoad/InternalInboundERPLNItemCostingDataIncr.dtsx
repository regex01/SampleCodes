                sb.AppendLine((("<tr><td><p><b><font size=\"4\">" + sPkgName + " <font color=\"#") + sColor + "\">") + sStatus + "</font></font></b></td></tr>");
                sb.AppendLine("<tr><td width=\"100%\" bgcolor=\"#0B6BE1\"><font color=\"#FFFFFF\"><b>Package Execution Summary</b></font></td></tr>");
