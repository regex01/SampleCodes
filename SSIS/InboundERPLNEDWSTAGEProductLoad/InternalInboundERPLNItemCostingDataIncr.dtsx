            /////////////////////////////////////////// 
            //Loop Through Warnings 
            /////////////////////////////////////////// 
            string sWarningHTML = string.Empty;
            sPrevSource = string.Empty;

            foreach (AlertInfo alertInfo in alertList)
            {
                if (alertInfo.AlertType != AlertType.WarningAlert)
